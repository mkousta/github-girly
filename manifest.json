{
  "name": "Github Girly",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Github Girly",
  "homepage_url": "https://github.com/mkousta/github-girly",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.github.com/*"],
      "js": ["script.js"]
    }
  ]
}